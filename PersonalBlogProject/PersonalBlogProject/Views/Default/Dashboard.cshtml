
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<!DOCTYPE html>
<html>
<head>

    <!-- bootstrap-css -->
    <link rel="stylesheet" href="~/admin/css/bootstrap.min.css">
    <!-- //bootstrap-css -->
    <!-- Custom CSS -->
    <link href="~/admin/css/style.css" rel='stylesheet' type='text/css' />
